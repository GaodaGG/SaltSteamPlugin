{
  "configs": [
    {
      "title": "Config",
      "config": "config.json",
      "preferences": [
        {
          "key": "songFormat",
          "type": "edittext",
          "title": "编辑songFormat",
          "default_value": "{artist} - {title}"
        },
        {
          "key": "cachedSteamworksClassName",
          "type": "edittext",
          "title": "缓存的 Steamworks 类名",
          "summary": "自动生成的缓存，通常无需手动修改",
          "default_value": ""
        },
        {
          "key": "cachedSpwInfo",
          "type": "edittext",
          "title": "缓存的 SPW 版本信息",
          "summary": "自动生成的缓存，通常无需手动修改",
          "default_value": ""
        },
        {
          "type": "button",
          "title": "强制重新扫描并刷新缓存",
          "on_click": "com.gg.SaltSteamPlugin.SteamworksCatcher.refreshSteamworks"
        }
      ]
    }
  ]
}